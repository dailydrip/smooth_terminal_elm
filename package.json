{
  "name": "smooth_terminal_elm",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build-api":
      "graphqelm http://localhost:4000/graphql --base SmoothTerminal",
    "build-css":
      "node-sass-chokidar --include-path ./src --include-path ./node_modules src/main.scss -o src",
    "watch-css":
      "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/main.scss -o src --watch --recursive",
    "build": "node scripts/build.js",
    "start": "node scripts/start.js",
    "package": "elm-package",
    "make": "elm-make",
    "repl": "elm-repl",
    "reactor": "elm-reactor",
    "test": "elm-test"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^8.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.3.1",
    "clean-webpack-plugin": "^0.1.18",
    "connect-history-api-fallback": "^1.5.0",
    "css-loader": "^0.28.9",
    "dotenv": "^5.0.0",
    "elm": "^0.18.0",
    "elm-hot-loader": "0.5.4",
    "elm-test": "^0.18.12",
    "elm-webpack-loader": "^4.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "graphqelm": "^3.1.12",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.4",
    "minimist": "1.2.0",
    "normalize-scss": "^7.0.1",
    "postcss-loader": "2.1.0",
    "purecss": "^0.6.2",
    "react-dev-utils": "^5.0.0",
    "react-error-overlay": "^4.0.0",
    "string-replace-loader": "^1.3.0",
    "style-loader": "^0.20.2",
    "sw-precache-webpack-plugin": "^0.11.4",
    "typeplate-starter-kit": "^3.0.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "node-sass-chokidar": "^1.3.3"
  }
}
